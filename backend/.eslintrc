{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module"
    },
    "rules": {
        "no-empty": 0,
        "no-console": 0,
        "import/named": 0,
        "global-require": 0,
        "prefer-destructuring": 0
    }
}
